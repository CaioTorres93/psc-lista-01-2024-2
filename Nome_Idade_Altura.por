programa {
  funcao inicio() {

    cadeia nomeUsuario 
    real nomeAltura 
    inteiro nomeIdade 

    escreva("Escreva seu nome:")
    leia(nomeUsuario)

    escreva("Escreva Altura:")
    leia(nomeAltura)

    escreva("Idade:")
    leia(nomeIdade)

    escreva(nomeUsuario, "\n", nomeAltura, "\n", nomeIdade)

    
  }
}
